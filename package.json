{
  "name": "angular2-toaster",
  "version": "0.0.1-alpha",
  "description": "An Angular 2 Toaster Notification library based on AngularJS-Toaster",
  "main": "angular2-toaster.ts",
  "scripts": {
    "prepublish": "tsc",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "lite": "lite-server",
    "start": "concurrent \"npm run tsc:w\" \"npm run lite\" "
  },
  "keywords": [
      "angular",
      "angular2",
      "angularjs-toaster",
      "angular2-toaster",
      "toaster",
      "notification"
  ],
  "author": "Stabzs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/stabzs/Angular2-Toaster.git"
  },
  "typings": "./angular2-toaster.d.ts",
  "dependencies": {
    "angular2": "^2.0.0-beta.3",
    "systemjs": "^0.19.20",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.33.3",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.0",
    "zone.js": "^0.5.11"
  },
  "devDependencies": {
    "concurrently": "^1.0.0",
    "lite-server": "^2.0.1",
    "typescript": "^1.7.5"
  }
}
